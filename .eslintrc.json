{
  "env": {
    "es6": true,
    "node": true
  },
  "extends": ["airbnb-base"],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "rules": {
    "no-undef": "error",
    "linebreak-style": "off",
    "import/prefer-default-export": "off",
    "arrow-parens": "off",
    "implicit-arrow-linebreak": "off",
    "object-curly-newline": "off",
    "no-underscore-dangle": "off",
    "operator-linebreak": "off"
  }
}
